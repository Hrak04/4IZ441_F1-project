###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf schema:Vehicle .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Circuit
:Circuit rdf:type owl:Class ;
         rdfs:subClassOf schema:Place .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Driver
:Driver rdf:type owl:Class ;
        rdfs:subClassOf foaf:Person .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#DriverAssignment
:DriverAssignment rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :assignedDriver ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Driver
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :assignedTeam ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Team
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :assignmentSeason ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Season
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :usesCar ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Car
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :driverNumber ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:integer
                                  ] .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Race
:Race rdf:type owl:Class ;
      rdfs:subClassOf schema:Event ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasCircuit ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Circuit
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :partOfSeason ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Season
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :winnerAssignment ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :DriverAssignment
                      ] .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Season
:Season rdf:type owl:Class ;
        owl:hasKey ( :seasonYear
                   ) .


###  http://www.semanticweb.org/hrak04/ontologies/f1_project#Team
:Team rdf:type owl:Class ;
      rdfs:subClassOf schema:Organization .


